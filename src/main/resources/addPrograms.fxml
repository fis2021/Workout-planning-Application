<?xml version="1.0" encoding="UTF-8"?>


<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="20" prefHeight="600" prefWidth="800" vgap="20" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.loose.fis.sre.controllers.AddProgramsController">
<padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
</padding>

<Label text="Program name" GridPane.columnIndex="0" GridPane.rowIndex="1">
    <font>
        <Font size="30.0" />
    </font></Label>

<TextField fx:id="name" GridPane.columnIndex="1" GridPane.rowIndex="1">
    <font>
        <Font size="20.0" />
    </font></TextField>

<Label text="Duration (days)" GridPane.columnIndex="0" GridPane.rowIndex="2">
    <font>
        <Font size="30.0" />
    </font></Label>

<TextField fx:id="duration" GridPane.columnIndex="1" GridPane.rowIndex="2">
    <font>
        <Font size="20.0" />
    </font></TextField>


<Label text="Intensity" GridPane.columnIndex="0" GridPane.rowIndex="3">
    <font>
        <Font size="30.0" />
    </font></Label>

<ChoiceBox fx:id="intensity" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
</ChoiceBox>


<HBox alignment="bottom_right" spacing="10" GridPane.columnIndex="0" GridPane.rowIndex="4">
    <Button onAction="#handleCancelButtonAction" text="Back">
        <font>
            <Font size="20.0" />
        </font></Button>
</HBox>

<HBox alignment="bottom_right" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="4">
    <Button onAction="#handleAddButtonAction" text="Add">
        <font>
            <Font size="20.0" />
        </font></Button>
</HBox>
<columnConstraints>
    <ColumnConstraints />
    <ColumnConstraints />
</columnConstraints>
<rowConstraints>
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
    <RowConstraints />
</rowConstraints>
</GridPane>