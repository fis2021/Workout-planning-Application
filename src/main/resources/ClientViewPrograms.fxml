<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #9f6060;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.loose.fis.sre.controllers.ClientVPController">
    <children>
        <ImageView fitHeight="400.0" fitWidth="600.0" layoutX="-1.0" layoutY="70.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@Images/a.jpg" />
            </image>
        </ImageView>
        <Text fill="#400e0e" layoutX="72.5" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Workout programs for you" textAlignment="CENTER" wrappingWidth="455.0">
            <font>
                <Font name="Impact" size="38.0" />
            </font>
        </Text>
        <TableView fx:id="table" layoutX="50.0" layoutY="135.0" prefHeight="200.0" prefWidth="500.0">
            <columns>
                <TableColumn fx:id="colname" prefWidth="166.66" text="Program name" />
                <TableColumn fx:id="colintensity" prefWidth="166.66" text="Intensity" />
            <TableColumn fx:id="colduration" prefWidth="166.66" text="Duration" />
            </columns>
        </TableView>
        <Button layoutX="450.0" layoutY="399.0" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #8a4242;" text="Back" textFill="WHITE">
            <font>
                <Font name="Times New Roman" size="16.0" />
            </font></Button>
        <Button layoutX="50.0" layoutY="388.0" mnemonicParsing="false" onAction="#handleWorkoutButton" prefHeight="50.0" prefWidth="150.0" style="-fx-background-color: #8a4242;" text="Workout" textFill="#fff7f7">
            <font>
                <Font name="Times New Roman" size="20.0" />
            </font>
        </Button>
        <Button layoutX="248.0" layoutY="388.0" mnemonicParsing="false" onAction="#handleEnrollButton" prefHeight="50.0" prefWidth="150.0" style="-fx-background-color: #8a4242;" text="Enroll" textFill="#fff7f7">
            <font>
                <Font name="Times New Roman" size="20.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
